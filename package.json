{
  "name": "oscalview",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "npm run build:wasm",
    "dev": "next dev --turbopack",
    "build": "npm run build:wasm && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "pre-commit": "npm run format && npm run lint && npm run test:all",
    "build:wasm": "cd wasm && make build",
